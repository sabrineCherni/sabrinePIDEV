<!DOCTYPE html>

<html>
<head>

    <meta name="viewport" content="width=device-width" />
    <title></title>

</head>
<body>

    <a href="@Url.Action("Create", "Employe")"> add new employe</a>

    <table class="table">
        <tr>
            <th>id</th>
            <th>nom</th>
            <th>prenom</th>
            <th>email</th>
        </tr>
        @foreach (var p in ViewBag.result)
        {
        <tr>
            <td>@p.id</td>
            <td>@p.nom</td>
            <td>@p.prenom</td>
            <td>@p.email</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = p.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = p.id })
            </td>
            
        </tr>

        }

    </table>


</body>
</html>

